    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run index.ts",
            "type": "node",
            "request": "launch",
            "args": [
                "index.ts"
            ],
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "cwd": "${workspaceRoot}/examples",
            "protocol": "inspector",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Run QuickStart",
            "type": "node",
            "request": "launch",
            "args": [
                "quick-start.ts"
            ],
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "cwd": "${workspaceRoot}/examples",
            "protocol": "inspector",
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}